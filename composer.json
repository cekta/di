{
  "name": "cekta/di",
  "description": "psr/container implementation for humans",
  "type": "library",
  "keywords": [
    "psr-11",
    "psr container",
    "di",
    "dependency injection",
    "container"
  ],
  "require": {
    "psr/container": ">=2.0",
    "php": ">=8.2",
    "cekta/di-module": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "squizlabs/php_codesniffer": "@stable",
    "phpstan/phpstan": "@stable",
    "infection/infection": "@stable"
  },
  "autoload": {
    "psr-4": {
      "Cekta\\DI\\": "src/"
    }
  },
  "provide": {
    "psr/container-implementation": "^2.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Cekta\\DI\\Test\\": "tests/"
    }
  },
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Evgeniy Kuvshinov",
      "email": "smpl@itmywork.com"
    }
  ],
  "scripts": {
    "test": [
      "@composer update",
      "@phpcs",
      "@phpstan",
      "phpunit",
      "@infection"
    ],
    "phpcs": "./vendor/bin/phpcs",
    "phpstan": "./vendor/bin/phpstan analyse --memory-limit=-1",
    "phpunit": "./vendor/bin/phpunit",
    "infection": "./vendor/bin/infection --show-mutations"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
