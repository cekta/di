{
  "name": "cekta/di",
  "description": "psr/container implementation for humans",
  "type": "library",
  "keywords": [
    "psr-11",
    "psr container",
    "di",
    "dependency injection",
    "container"
  ],
  "require": {
    "psr/container": ">=2.0",
    "php": ">=8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable",
    "squizlabs/php_codesniffer": "@stable",
    "phpstan/phpstan": "@stable",
    "infection/infection": "@stable"
  },
  "autoload": {
    "psr-4": {
      "Cekta\\DI\\": "src/"
    }
  },
  "provide": {
    "psr/container-implementation": "^2.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Cekta\\DI\\Test\\": "tests/"
    }
  },
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Evgeniy Kuvshinov",
      "email": "smpl@itmywork.com"
    }
  ],
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
